{
  "name": "@gcoredev/as-jwt",
  "version": "0.0.1",
  "description": "AssemblyScript package that provides simple jws handling for jwt tokens/",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "asbuild:untouched": "asc assembly/index.ts --target debug",
    "asbuild:optimized": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "test": "asp --verbose",
    "test:ci": "asp --summary",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/godronus/as-jwt.git"
  },
  "keywords": [
    "assemblyscript",
    "hmac",
    "sha256",
    "jwt",
    "jws"
  ],
  "author": "Gordon Farquharson <godronus@yahoo.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/godronus/as-jwt/issues"
  },
  "homepage": "https://github.com/godronus/as-jwt#readme",
  "devDependencies": {
    "@as-pect/cli": "^8.0.1",
    "@assemblyscript/loader": "^0.27.34",
    "assemblyscript": "^0.27.34",
    "husky": "^9.0.11"
  },
  "dependencies": {
    "assemblyscript-json": "^1.1.0"
  }
}
